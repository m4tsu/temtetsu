{
  "name": "nextjs-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "run-s -c lint:*",
    "lint:eslint": "next lint",
    "lint:prettier": "prettier --check src/**/*.{ts,tsx,js,jsx,json,md}",
    "lint:tsc": "tsc --noEmit",
    "lint-fix": "run-s -c lint-fix:*",
    "lint-fix:eslint": "next lint --fix",
    "lint-fix:prettier": "prettier --write src/**/*.{ts,tsx,js,jsx,json,md}",
    "prepare": "husky install",
    "synchronize-tem-data": "ts-node --project tsconfig.scripts.json ./scripts/synchronize-tem-data.ts"
  },
  "dependencies": {
    "@google-cloud/local-auth": "^2.1.1",
    "@googleapis/sheets": "^4.0.2",
    "@maael/temtem-types": "^1.0.2",
    "@next/font": "13.4.5",
    "@types/node": "20.3.1",
    "@types/react": "18.2.12",
    "@types/react-dom": "18.2.5",
    "clsx": "^1.2.1",
    "eslint": "8.42.0",
    "eslint-config-next": "13.4.5",
    "fast-csv": "^4.3.6",
    "googleapis": "^105.0.0",
    "next": "13.4.5",
    "react": "18.2.0",
    "react-aria": "^3.25.0",
    "react-aria-components": "^1.0.0-alpha.4",
    "react-dom": "18.2.0",
    "tailwind-merge": "^1.13.1",
    "tailwindcss-animate": "^1.0.6",
    "typescript": "5.1.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "autoprefixer": "^10.4.14",
    "daisyui": "^3.1.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "tailwindcss": "^3.3.2",
    "ts-node": "^10.9.1"
  }
}
